<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>
		<!-- http://www.mybatis.org/mybatis-3/ko/configuration.html -->
		<setting name="cacheEnabled" value="true" />
		<setting name="lazyLoadingEnabled" value="false" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="defaultExecutorType" value="SIMPLE" />
		<setting name="defaultStatementTimeout" value="25000" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<setting name="jdbcTypeForNull" value="VARCHAR" />
		<!-- <setting name="callSettersOnNulls" value="true" /> -->
	</settings>

	<typeAliases>
		<typeAlias alias="noticeDTO" type="shop.carrental.domain.NoticeDTO" />
		<typeAlias alias="usersDTO" type="shop.carrental.domain.UsersDTO"/>
		<typeAlias alias="inquiryDTO" type="shop.carrental.domain.InquiryDTO"/>
		<typeAlias alias="answerDTO" type="shop.carrental.domain.AnswerDTO"/>
		<typeAlias alias="faqDTO" type="shop.carrental.domain.FaqDTO"/>
		<typeAlias alias="carDTO" type="shop.carrental.domain.CarDTO"/>
		<typeAlias alias="mfgcoDTO" type="shop.carrental.domain.MfgcoDTO"/>
		<typeAlias alias="branchDTO" type="shop.carrental.domain.BranchDTO"/>
		<typeAlias alias="shortCarDTO" type="shop.carrental.domain.ShortCarDTO"/>
		<typeAlias alias="segmentDTO" type="shop.carrental.domain.SegmentDTO"/>
		<typeAlias alias="carDTO" type="shop.carrental.domain.CarDTO"/>
		<typeAlias alias="carVO" type="shop.carrental.domain.CarVO"/>
		<typeAlias alias="appointDTO" type="shop.carrental.domain.AppointDTO"/>
		<typeAlias alias="shortCarVO" type="shop.carrental.domain.ShortCarVO"/>
		<typeAlias alias="insuranceDTO" type="shop.carrental.domain.InsuranceDTO"/>
	</typeAliases>

</configuration>